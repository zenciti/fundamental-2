jumlah_buku = 10
print( "ibu berkata, baca semua bukumu")

jumlah_buku_yg_sudah_dibaca = 0

print(f"jumlah buku yg dibaca = {jumlah_buku_yg_sudah_dibaca}")

for jumlah_buku_yg_sudah_dibaca in range (1,jumlah_buku+1):
    print (f"jumlah buku yg sudah dibaca {jumlah_buku_yg_sudah_dibaca}")

print(f"jumlah buku yg sudah dibaca = {jumlah_buku_yg_sudah_dibaca}")
